<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Image\app2.ico</ApplicationIcon>
    <Title>CORVOLIVE ULTRA CLEANER</Title>
    <Authors>FANCESCO CORVETTI</Authors>
    <Company>CrowLand</Company>
    <PackageIcon>8541770_broom_clean_icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="8541770_broom_clean_icon.png" />
    <None Remove="app2.ico" />
    <None Remove="Image\app2.ico" />
    <None Remove="Image\cloud-computing.png" />
    <None Remove="Image\timeline.png" />
    <None Remove="Image\web-programming.png" />
    <None Remove="timeline.png" />
    <None Remove="web-programming.png" />
